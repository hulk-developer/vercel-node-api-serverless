{
  "name": "vercel-node-api-serverless",
  "version": 2,
  "env": {
    "MONGODB_URI": "@my-mongodb-uri"
  },
  "functions": {
    "api/users.js": {
      "excludeFiles": ".env"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/tweet",
      "destination": "https://twitter.com/webdevcaptain",
      "permanent": false
    }
  ]
}
